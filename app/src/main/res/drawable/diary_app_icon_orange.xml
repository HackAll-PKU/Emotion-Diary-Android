<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/themeColorOrange" />
            <padding
                android:bottom="30dp"
                android:left="40dp"
                android:right="40dp"
                android:top="30dp" />
        </shape>
    </item>
    <item>
        <bitmap
            android:antialias="true"
            android:dither="true"
            android:filter="true"
            android:src="@drawable/diary" />
    </item>
</layer-list>
